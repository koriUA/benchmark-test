  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "jest",
    "test:watch": "jest --watchAll --runInBand --verbose",
    "test:update": "jest --updateSnapshot",
    "test:clear": "jest --clearCache",
    "format": "prettier --write \"**/*.{scss,css,js,jsx,md,ts}\"",
    "format:diff": "prettier --list-different \"**/*.{scss,css,js,jsx,md,ts}\"",
    "lint": "yarn lint:javascript && yarn lint:stylelint",
    "lint:javascript": "eslint --ext .jsx --ext .js .",
    "lint:stylelint": "stylelint './src/*/*.scss' --syntax scss --ignorePath .gitignore"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "yarn test --silent --watchAll=false"
    }
  },
  "stylelint": {
    "extends": "stylelint-config-recommended-scss",
    "plugins": [
      "stylelint-scss"
    ]
  },
  "lint-staged": {
    "*.{jsx,js}": [
      "eslint --fix"
    ],
    "*.{scss,css}": [
      "yarn lint:stylelint"
    ],
    "*.{scss,css,js,md,jsx,json}": [
      "yarn format",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
