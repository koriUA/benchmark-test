{
  "extends": "./node_modules/@decibel/components/.babelrc",
  "plugins": [
    ["module-resolver", {
      "root": ".",
      "alias": {
        "common": "./src/common/",
        "localization": "./src/common/localization/",
        "utilities": "./src/common/utilities/",
        "components": "./src/components/",
        "store": "./src/store/",
        "routes": "./src/routes/",
        "services": "./src/services/"
      }
    }], [
      "@babel/plugin-transform-runtime",
